<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %}{% trans %}Welcome!{% endtrans  %}{% endblock %}</title>
        {% block meta %}
            <meta name="description" content="">
            <meta name="viewport" content="width=device-width, initial-scale=1">
        {% endblock %}

        {% block stylesheets %}
            {% stylesheets 'bundles/bookshelfapp/css/normalize.css' output='compiled/css/*.css' %}
                <link rel="stylesheet" href="{{ asset_url }}"/>
            {% endstylesheets %}
        {% endblock %}

        {% image 'bundles/bookshelfapp/images/favicon.ico' output='compiled/images/*.ico' %}
            <link rel="icon" type="image/x-icon" href="{{ asset_url }}" />
        {% endimage %}

        {% block head_javascripts %}
            {% javascripts 'bundles/bookshelfapp/js/vendor/modernizr-2.6.2.min.js' output='compiled/js/*.js' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    {% endblock %}
</head>
<body>
{% block body %}
{% endblock %}

{% block javascripts %}
{% endblock %}

{% block analytics %}{% endblock %}
</body>
</html>